{
  "name": "expo-react-native-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "expo start -c --dev-client",
    "android": "expo start -c --android --dev-client",
    "ios": "expo start -c --ios --dev-client",
    "prebuild": "npx expo prebuild",
    "local": "./scripts/run-local.sh",
    "build:artifact": "./scripts/build-artifact.sh",
    "build:dev": "./scripts/build-dev.sh",
    "build:staging": "./scripts/build-staging.sh",
    "build:production": "./scripts/build-production.sh",
    "submit:dev": "./scripts/submit-dev.sh",
    "submit:staging": "./scripts/submit-staging.sh",
    "submit:production": "./scripts/submit-production.sh",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "lint:lockfile": "npx lockfile-lint --path package-lock.json --type npm --allowed-hosts npm --validate-https",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "validate": "npm run check && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .expo node_modules ios android && npm cache clean --force && npm run setup",
    "setup": "npm ci && npx --no-install allow-scripts",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@rn-primitives/checkbox": "^1.2.0",
    "@rn-primitives/collapsible": "^1.2.0",
    "@rn-primitives/dialog": "^1.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/radio-group": "^1.2.0",
    "@rn-primitives/select": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/switch": "^1.2.0",
    "@rn-primitives/tabs": "^1.2.0",
    "@rn-primitives/toggle": "^1.2.0",
    "@rn-primitives/toggle-group": "^1.2.0",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.29",
    "expo-auth-session": "~7.0.10",
    "expo-constants": "~18.0.11",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.10",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-actions-sheet": "^10.1.1",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@lavamoat/allow-scripts": "^3.4.1",
    "@lavamoat/preinstall-always-fail": "^2.1.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^7.13.1",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "msw": "^2.11.5",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "sharp": "^0.34.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "lavamoat": {
    "allowScripts": {
      "$root$": false,
      "@lavamoat/preinstall-always-fail": false,
      "ljharb-monorepo-symlink-test": false,
      "msw": true,
      "sharp": true
    }
  }
}
